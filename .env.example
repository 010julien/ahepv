# EmailJS configuration (copy this file to .env.local and fill the values)
# Create a service and templates on https://dashboard.emailjs.com/

VITE_EMAILJS_PUBLIC_KEY=YOUR_PUBLIC_KEY
VITE_EMAILJS_SERVICE_ID=YOUR_SERVICE_ID

# Optional: you can rename templates; otherwise defaults will be used
# Recommended template variables:
#  - to_email (destination)
#  - from_email
#  - from_name
#  - subject
#  - message
#  - phone
#  - event_title (for events)
#  - tickets (for events)
#  - interests (for volunteer)
#  - availability (for volunteer)
VITE_EMAILJS_TEMPLATE_CONTACT=contact_template
VITE_EMAILJS_TEMPLATE_VOLUNTEER=volunteer_template
VITE_EMAILJS_TEMPLATE_EVENT=event_registration_template
